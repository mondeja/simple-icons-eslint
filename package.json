{
  "name": "simple-icons-eslint",
  "version": "0.1.1",
  "description": "Simple Icons ESLint base configuration for all repositories",
  "homepage": "https://simpleicons.org",
  "main": "index.js",
  "scripts": {
    "test": "eslint-config-prettier index.js",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write . && eslint --fix .",
    "prepare": "is-ci || husky install"
  },
  "keywords": [
    "eslint",
    "simple-icons"
  ],
  "author": "Simple Icons Collaborators",
  "license": "CC0-1.0",
  "devDependencies": {
    "eslint": "8.24.0",
    "eslint-config-prettier": "8.5.0",
    "husky": "8.0.1",
    "is-ci": "3.0.1",
    "prettier": "2.7.1"
  },
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/simple-icons"
  },
  "bugs": {
    "url": "https://github.com/simple-icons/simple-icons-eslint/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/simple-icons/simple-icons-eslint.git"
  }
}
